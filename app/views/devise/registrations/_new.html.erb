<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), html: { class: 'dialog-form' }) do |f| %>
  <%= devise_error_messages! %>
  <%= f.input :username %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.submit "Register", class: 'btn btn-primary' %>
<% end %>